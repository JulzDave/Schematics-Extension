{
	"name": "mataf-schematics",
	"displayName": "Mataf Schematics",
	"description": "Mataf schemtics installer.",
	"icon": "assets/images/fibi-icon.png",
	"publisher": "Mataf-Research-and-Development",
	"author": "Julian Dave Shikui",
	"repository": {
		"type": "git",
		"url": "https://github.com/JulzDave/Vscode-extension---Mataf-NX-NestJS-schematic.git"
	},
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:mataf-schematics.nxNestJSSchematic"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "mataf-schematics.nxNestJSSchematic",
				"title": "Mataf NX-NestJS schematic"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.43.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
